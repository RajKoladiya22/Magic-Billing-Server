{
  "name": "cpm-backend-prisma",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn src/server.ts",
    "build": "tsc -p tsconfig.json && cp -R views dist/views",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "prisma:generate": "prisma generate",
    "migration:generate": "npx prisma migrate dev --name product_img",
    "migration:deploy": "npx prisma migrate deploy",
    "migration:reset": "prisma migrate reset --force",
    "migration:resolve": "npx prisma migrate resolve --applied <migration_name>",
    "migration:status": "npx prisma migrate status",
    "seed": "ts-node prisma/seed.ts",
    "start": "node dist/server.js",
    "start:dev": "cross-env NODE_ENV=development concurrently \"tsc -w\" \"nodemon dist/server.js\"",
    "start:prod": "cross-env NODE_ENV=production pm2 start dist/server.js --name MagicBilling",
    "postinstall": "prisma generate",
    "test": "jest",
    "prisma:gen": "dotenv -e .env.$NODE_ENV -- npx prisma generate",
    "prisma:migrate": "dotenv -e .env.$NODE_ENV -- npx prisma migrate dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.7.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.6.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cron": "^4.3.1",
    "crypto": "^1.0.1",
    "csv-parser": "^3.2.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "ejs": "^3.1.10",
    "envalid": "^8.0.0",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash-es": "^4.17.21",
    "module-alias": "^2.2.3",
    "ms": "^2.1.3",
    "multer": "^2.0.0",
    "nodemailer": "^7.0.3",
    "rimraf": "^5.0.10",
    "uuid": "^9.0.1",
    "vercel": "^41.0.2",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.25.30"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/cloudinary-uploadwidget-browser": "^2.3.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.13",
    "@types/node": "^22.15.27",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.7.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "_moduleAliases": {
    "@core": "dist/core/*",
    "@config": "dist/config",
    "@domains": "dist/domains",
    "@routes": "dist/routes"
  }
}
